<Window x:Class="TrainTickets.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrainTickets.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1080"
        Style="{StaticResource {x:Type Window}}" 
        WindowStyle="None" AllowsTransparency="True" Icon="/View/train.jpg"
        >

    <Grid>
        <Frame Content="{Binding NavigationService.CurrentView}" NavigationUIVisibility="Hidden"/>
    </Grid>

</Window>
